  <script>
    document.addEventListener('DOMContentLoaded', function () {
      const leftDiv = document.querySelector('.left-div');
      const rightDiv = document.querySelector('.right-div');
      const tooltipLeft = document.querySelector('.tooltip-left');
      const tooltipRight = document.querySelector('.tooltip-right');

      let leftTimeout, rightTimeout;

      leftDiv.addEventListener('mouseenter', function (event) {
        const { top, left, height } = event.target.getBoundingClientRect();
        tooltipLeft.style.top = `${top + height}px`;
        tooltipLeft.style.left = `${left}px`;
        tooltipLeft.style.display = 'block';
        document.querySelector('.tooltip-left .scrollable-div').style.display = 'block';
      });

      leftDiv.addEventListener('mouseleave', function () {
        leftTimeout = setTimeout(() => {
          tooltipLeft.style.display = 'none';
          document.querySelector('.tooltip-left .scrollable-div').style.display = 'none';
        }, 200);
      });

      tooltipLeft.addEventListener('mouseenter', function () {
        clearTimeout(leftTimeout);
      });

      tooltipLeft.addEventListener('mouseleave', function () {
        tooltipLeft.style.display = 'none';
        document.querySelector('.tooltip-left .scrollable-div').style.display = 'none';
      });

      rightDiv.addEventListener('mouseenter', function (event) {
        const { top, left, height } = event.target.getBoundingClientRect();
        tooltipRight.style.top = `${top + height}px`;
        tooltipRight.style.left = `${left}px`;
        tooltipRight.style.display = 'block';
        document.querySelector('.tooltip-right .scrollable-div').style.display = 'block';
      });

      rightDiv.addEventListener('mouseleave', function () {
        rightTimeout = setTimeout(() => {
          tooltipRight.style.display = 'none';
          document.querySelector('.tooltip-right .scrollable-div').style.display = 'none';
        }, 200);
      });

      tooltipRight.addEventListener('mouseenter', function () {
        clearTimeout(rightTimeout);
      });

      tooltipRight.addEventListener('mouseleave', function () {
        tooltipRight.style.display = 'none';
        document.querySelector('.tooltip-right .scrollable-div').style.display = 'none';
      });
    });
  </script>


