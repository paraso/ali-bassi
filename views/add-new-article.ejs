      <script>
        const scrollableForm = document.getElementById("scrollableForm");
      
        let isMouseDown = false;
        let startX;
        let startY;
        let scrollLeft;
        let scrollTop;
      
        scrollableForm.addEventListener("mousedown", (e) => {
          isMouseDown = true;
          startX = e.pageX - scrollableForm.offsetLeft;
          startY = e.pageY - scrollableForm.offsetTop;
          scrollLeft = scrollableForm.scrollLeft;
          scrollTop = scrollableForm.scrollTop;
        });
      
        document.addEventListener("mouseup", () => {
          isMouseDown = false;
        });
      
        document.addEventListener("mousemove", (e) => {
          if (isMouseDown) {
            const x = e.pageX - scrollableForm.offsetLeft;
            const y = e.pageY - scrollableForm.offsetTop;
            const moveX = x - startX;
            const moveY = y - startY;
            scrollableForm.scrollLeft = scrollLeft - moveX;
            scrollableForm.scrollTop = scrollTop - moveY;
          }
        });
      
        scrollableForm.addEventListener("wheel", (e) => {
          e.preventDefault();
          scrollableForm.scrollLeft += e.deltaX;
          scrollableForm.scrollTop += e.deltaY;
        });
      </script>




      



















